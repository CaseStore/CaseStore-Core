{% extends 'CaseStoreBundle::base.html.twig' %}


{% block title %}New Project{% endblock %}



{% block main %}

    <ol class="breadcrumb">
        <li class="active">New Project</li>
    </ol>


    <section class="">

        <form class="styled" action="" method="post" {{ form_enctype(form) }}>
            {{ form_errors(form) }}

            {{ form_rest(form) }}

            <input type="submit" value="Create New Project" />
    
        </form>

    </section>



{%  endblock %}

{% block javascript %}
    {% javascripts
    '@CaseStoreBundle/Resources/public/js/newProjectSlug.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

